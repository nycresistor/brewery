#!/usr/bin/env python

import zmq
import sys
import random

port = "5559"
context = zmq.Context()
socket = context.socket(zmq.REQ)
socket.connect ("tcp://localhost:%s" % port)
client_id = random.randrange(1,10005)
socket.send ("templs %s" % client_id)
#  Get the reply.
celsius = socket.recv()
celsius = float(celsius)
fahrenheit = 9.0/5.0 * celsius + 32
print fahrenheit
